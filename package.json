{
  "name": "ecommerce-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:init": "./scripts/init-db.sh",
    "docs": "echo 'Use api-tests.http with REST Client extension'",
    "test:api": "echo 'Open api-tests.http in VS Code with REST Client extension'",
    "test:rate-limit": "node tests/test-rate-limiting.js",
    "test:security-headers": "node tests/security-headers-test.js",
    "test:validation": "node tests/test-validation.js",
    "test:access-security": "node tests/test-access-security.js",
    "test:payment-intent": "NODE_ENV=development node tests/payment-intent-reuse.test.js",
    "test:payment-edge-cases": "NODE_ENV=development node tests/payment-intent-edge-cases.test.js",
    "test:payment-security": "NODE_ENV=development node tests/payment-amount-validation.test.js",
    "test:webhook-security": "NODE_ENV=development node tests/webhook-security.test.js",
    "test:payment-test-mode": "NODE_ENV=development PAYMENTS_TEST_MODE=true node tests/payment-test-mode-security.test.js",
    "test:auth-security": "NODE_ENV=development node tests/auth-security-simple.test.js",
    "test:webhook-race": "NODE_ENV=development node tests/webhook-idempotence-race.test.js",
    "test:webhook-integration": "NODE_ENV=development node tests/webhook-idempotence-integration.test.js",
    "security:audit": "echo 'Use: npm run test:security-headers && open https://securityheaders.com'",
    "security:all": "npm run test:rate-limit && npm run test:security-headers && npm run test:validation && npm run test:access-security && npm run test:webhook-security && npm run test:auth-security",
    "test:payments": "node scripts/test-runner.js",
    "test:payments:manual": "npm run test:payment-intent && npm run test:payment-edge-cases && npm run test:payment-security && npm run test:webhook-security"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.15.0",
    "@stripe/react-stripe-js": "^4.0.0",
    "@stripe/stripe-js": "^7.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  }
}
